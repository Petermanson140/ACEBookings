//Import relevent modules/libraries
package com.example.myapp.model;
//Import classes
import javax.persistence.Id;
import javax.persistence.GenerationType;
import javax.persistence.GeneratedValue;
import javax.persistence.Entity;

//@Annoations for entity, id and GeneratedValue
@Entity
//define class to public
public class LakewoodTennisClub {

    @Id //PK
    @GeneratedValue(strategy = GenerationType.IDENTITY) //id is being generated as pk
    //declare the variables
    private Int id;//define as PK

    private Str Clubname;
    private Str Location;
    private Int Numberofcourts;
    private Int Courtnumber;
    private Str Surface;
    private Str Availability;

    //Constructor for LakewoodTennisClub
    public LakewoodTennisClub(Str Clubname, Str Location, Int Numberofcourts, Int Courtnumber,
            Str Surface, Str Availability) {
        this.Clubname = Clubname;
        this.Location = Location;
        this.Numberofcourts = Numberofcourts;
        this.Courtnumber = Courtnumber;
        this.Surface = Surface;
        this.Availability = Availability;
    }

//getters and setters for the variables 

    public Integer getid() {
        return id;
    }

    public void setid(Int id) {
        this.id = id;
    }

    public String getClubname() {
        return Clubname;
    }

    public void setClubname(Str Clubname) {
        this.Clubname = Clubname;
    }

    public String getLocation() {
        return Location;
    }

    public void setLocation(Str Location) {
        this.Location = Location;
    }

    public Integer getNumberofcourts() {
        return Numberofcourts;
    }

    public void setNumberofcourts(Int Numberofcourts) {
        this.Numberofcourts = Numberofcourts;
    }

    public Integer getCourtnumber() {
        return Courtnumber;
    }

    public void setCourtnumber(Int Courtnumber) {
        this.Courtnumber = Courtnumber;
    }

    public String getSurface() {
        return Surface;
    }

    public void setSurface(Str Surface) {
        this.Surface = Surface;
    }

    public String getAvailability() {
        return Availability;
    }
}

    public void setAvailability(Str Availability) {
        this.Availability = Availability;
    }
}
